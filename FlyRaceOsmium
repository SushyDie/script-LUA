function Optimethod1()
    while _G.OptiFarm1 == true do
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(232.0892791748047, 35, -894.423583984375)
        wait(0.2)
        repeat
            OptiFarm1 = OptiFarm1 + 1
           game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(160335.421875, 396.5972900390625, 104614.515625)
           wait()
        until OptiFarm1 == _G.OptiFarmValue
        wait()
        OptiFarm1 = 0
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(200, -5, 104659)
        wait(_G.RebirthxOptiwait)
    end
end


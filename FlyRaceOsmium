--FarmTab
local label = FarmTab:CreateLabel("Other","˅ Free Magnet, Auto Orb, Auto Rebirth ˅")

local toggle = FarmTab:CreateToggle("Auto Orb", false, function (bool)
    getgenv().CollectOrb = bool

    task.spawn(function()
        while CollectOrb and task.wait() do
            pcall(function()
                for i,v in pairs(game.Workspace.Camera:GetChildren()) do
                    if v:IsA("Part") and v.Orb.Item.Image ~= "rbxassetid://11411247929" then
                        local hrp = lp.Character.HumanoidRootPart
                        v.Transparency = 1
                        v.CanCollide = false
                        hrp.CFrame = v.CFrame + Vector3.new(0, 5, 0)
                        task.wait(.1)
                    end
                end
            end)
        end
    end)
end)

local toggle = FarmTab:CreateToggle("Free Magnet", false, function (Value)
    _G.CollectOrb2 = Value
    CollectOrb22()
end)

local toggle = FarmTab:CreateToggle("Auto Rebirth", false, function (bool)
    getgenv().AutoRebirth = bool
    task.spawn(function()
        while AutoRebirth and task.wait() do
            if lp.PlayerGui.Main.SideTabs.Big.Rebirths.Main.Unchecked.Visible then
                click(lp.PlayerGui.Main.SideTabs.Big.Rebirths)
                task.wait(.5)
                click(lp.PlayerGui.Rebirths.Main.Bottom.Confirm)
                task.wait(.2)
                click(lp.PlayerGui.Rebirths.Main.Exit)
            end
        end
    end)
end)

